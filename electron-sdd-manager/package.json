{
  "name": "sdd-orchestrator",
  "version": "0.9.2",
  "description": "SDD Orchestrator - Spec-Driven Development workflow manager",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "electron .",
    "build": "tsc && vite build",
    "build:electron": "npm run build && electron-builder",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "npm run build && wdio run wdio.conf.ts",
    "test:e2e:watch": "wdio run wdio.conf.ts --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/express": "^5.0.5",
    "@types/qrcode": "^1.5.6",
    "@types/ssh2": "^1.15.5",
    "@types/ws": "^8.18.1",
    "@uiw/react-md-editor": "^4.0.8",
    "chokidar": "^5.0.0",
    "clsx": "^2.1.1",
    "electron-store": "^8.2.0",
    "express": "^5.1.0",
    "lucide-react": "^0.554.0",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "ssh2": "^1.17.0",
    "tailwind-merge": "^3.4.0",
    "ws": "^8.18.3",
    "zod": "^3.24.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "@wdio/cli": "^9.20.1",
    "@wdio/local-runner": "^9.20.1",
    "@wdio/mocha-framework": "^9.20.1",
    "@wdio/spec-reporter": "^9.20.0",
    "autoprefixer": "^10.4.22",
    "electron": "35.5.1",
    "electron-builder": "^24.13.3",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "tree-kill": "^1.2.2",
    "typescript": "~5.8.3",
    "vite": "^5.4.0",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^2.1.0",
    "wdio-electron-service": "^9.2.1"
  },
  "build": {
    "appId": "com.sdd-orchestrator.app",
    "productName": "SDD Orchestrator",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "!node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "../scripts",
        "to": "scripts",
        "filter": ["**/*"]
      },
      {
        "from": "resources",
        "to": "resources",
        "filter": ["**/*"]
      }
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "public/icon.icns"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "public/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "public/icon.png"
    }
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "utf-8-validate": "^6.0.5"
  }
}
